{
  "name": "storybook-addon-source",
  "version": "2.0.11",
  "description": "A panel that displays a code in a storybook",
  "license": "MIT",
  "files": [
    "dist/*",
    "register.js"
  ],
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "babel": "babel src/*.js --out-dir dist",
    "lint": "eslint --cache '*.js'",
    "release": "standard-version",
    "security": "npm audit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenshuai2144/storybook-addon-source"
  },
  "dependencies": {
    "@storybook/addons": "^5.2.6",
    "@storybook/components": "^5.2.6",
    "react": "^16.7.0",
    "react-syntax-highlighter": "^10.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.1.6",
    "@commitlint/config-conventional": "^7.1.2",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.11.1",
    "standard-version": "^8.0.1"
  },
  "peerDependencies": {
    "webpack": "^4.3.0"
  },
  "engines": {
    "node": ">= 6.9.0"
  }
}
